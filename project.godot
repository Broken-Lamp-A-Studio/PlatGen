; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="PlatGen"
run/main_scene="res://scenes/Menu.tscn"
boot_splash/image="res://boot.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[debug]

settings/gdscript/max_call_stack=1100
shapes/collision/shape_color=Color( 0, 0.6, 0.701961, 0.501961 )

[layer_names]

2d_physics/layer_1="Ground"
2d_physics/layer_2="player"
2d_physics/layer_3="Wall"
2d_physics/layer_4="enemy"

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="user://.PlatGen/logs/log.txt"
file_logging/max_log_files=25

[rendering]

environment/default_environment="res://default_env.tres"
